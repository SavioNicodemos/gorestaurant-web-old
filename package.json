{
  "name": "go-restaurant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve:ui": "vite preview",
    "serve:back": "json-server --watch server.json --port 3333",
    "lint:check": "pnpm dlx @biomejs/biome check src",
    "lint:fix": "pnpm dlx @biomejs/biome check --apply src",
    "tsc": "tsc",
    "test": "vitest watch",
    "test:no-watch": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^5.3.4",
    "styled-components": "^6.1.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@types/yup": "^0.26.37",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "axios-mock-adapter": "^1.22.0",
    "c8": "^8.0.1",
    "jsdom": "^23.0.1",
    "json-server": "^0.16.3",
    "ts-jest": "^29.1.1",
    "typescript": "~5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}