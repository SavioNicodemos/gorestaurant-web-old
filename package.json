{
  "name": "go-restaurant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve:ui": "vite preview",
    "serve:back": "json-server --watch server.json --port 3333",
    "lint:check": "pnpm dlx @biomejs/biome check src",
    "lint:fix": "pnpm dlx @biomejs/biome check --apply src",
    "ts:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "axios": "^1.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.21.1",
    "styled-components": "^6.1.6",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-modal": "^3.16.3",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.1",
    "axios-mock-adapter": "^1.22.0",
    "c8": "^9.0.0",
    "jsdom": "^23.0.1",
    "json-server": "^0.17.4",
    "ts-jest": "^29.1.1",
    "typescript": "~5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}